server {
	listen 8000;
    server_name example.com www.example.com;
    root /var/www/example;

	index index.html index.htm;

	location /test {
		allowed_methods GET;
		root /test/example1;
		autoindex on;
	}
}

server {
	listen 9000;
    server_name example2.com www.example2.com;
    root /var/www/example2;

    index index2.html index2.htm
}
